{
  "manifest_version": 2,
  "name": "Local Rush",
  "version": "1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "permissions": [
    {"socket": [
      "tcp-connect",
      "tcp-listen"]}],
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  }
}
